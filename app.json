{
  "expo": {
    "name": "Solex Trade",
    "slug": "solex-trade-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-optimized.png",
    "scheme": "solextradeapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/app-icon-optimized.png",
      "bundleIdentifier": "com.solex.tradeapp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background-black.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "icon": "./assets/images/app-icon-optimized.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.solex.tradeapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/app-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app-logo.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "739e4e91-c5e2-4c12-aacb-8c7d78b05413"
      }
    },
    "owner": "mezie_page"
  }
}
